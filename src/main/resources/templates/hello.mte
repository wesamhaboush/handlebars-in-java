<script id="some-id" type="text/x-handlebars-template">
    <table>
        <thead>
        <th>Username</th>
        <th>Real Name</th>
        <th>Email</th>
        </thead>
        <tbody>
        ${foreach users user}
        <tr>
            <td>${user.username}</td>
            <td>${user.firstName} ${user.lastName}</td>
            <td>${user.email}</td>
        </tr>
        ${end}
        </tbody>
    </table>
</script>