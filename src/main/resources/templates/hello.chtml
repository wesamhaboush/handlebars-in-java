<script id="some-id" type="text/x-handlebars-template">
    <table>
        <thead>
        <th>Username</th>
        <th>Real Name</th>
        <th>Email</th>
        </thead>
        <tbody>
        {% loop in $users as $user %}
        <tr>
            <td>{$user.username}</td>
            <td>{$user.first_name} {$user.last_name}</td>
            <td>{$user.email}</td>
        </tr>
        {% endloop %}
        </tbody>
    </table>
</script>
